{
  "name": "koa2",
  "version": "1.0.0",
  "description": "node koa2",
  "main": "app.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/app.js --watch",
    "dev_app": "npm run dev_server & npm run dev_static",
    "dev_server": "nodemon --harmony ./server/app",
    "dev_static": "webpack --watch --config ./static/build/webpack.dev.config.js",
    "start_static": "webpack --config ./static/build/webpack.prod.config.js",
    "start_server": "node ./server/app.js",
    "start": "npm run start_static & npm run start_server"
  },
  "keywords": [
    "koa2",
    "node"
  ],
  "dependencies": {
    "busboy": "^0.3.1",
    "ejs": "^2.6.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.0.0",
    "koa-session": "^5.12.2",
    "koa-session-minimal": "^3.0.4",
    "koa-static": "^3.0.0",
    "koa-views": "^6.2.0",
    "mongoose": "^5.6.10",
    "validator": "^11.1.0",
    "antd": "^3.10.9",
    "debug": "^2.6.0",
    "koa-send": "^3.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "author": "ivin",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "externals-dependencies": "^1.0.4",
    "koa-webpack": "^5.2.4",
    "koa-webpack-hot-middleware": "^1.0.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.4"
  }
}
